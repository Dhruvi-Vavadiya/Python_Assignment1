    "def check_first_last(nums):\n",
    "    if not nums:  # Handle empty list case\n",
    "        return False\n",
    "    return nums[0] == nums[-1]\n",
    "print(check_first_last([1, 2, 3, 4, 1]))  # True\n",
    "print(check_first_last([5, 6, 7, 8]))     # False\n",
    "print(check_first_last([]))                # False (empty list)"
   ]
