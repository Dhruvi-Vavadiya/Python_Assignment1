    "#11. calculate the sum of all number between 1 and given number\n",
    "def sum_upto(n):\n",
    "    return sum(range(1, n + 1))\n",
    "\n",
    "# Example usage:\n",
    "num = 10\n",
    "result = sum_upto(num)\n",
    "print(f\"Sum of numbers from 1 to {num} is {result}\")\n"
