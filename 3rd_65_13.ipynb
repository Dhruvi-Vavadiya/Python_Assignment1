    "List1 = [10, 20, 30, 40]\n",
    "reversed_list = []\n",
    "\n",
    "for i in range(len(List1) - 1, -1, -1):\n",
    "    reversed_list.append(List1[i])\n",
    "\n",
    "print(reversed_list)\n"
